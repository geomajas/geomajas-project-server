<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.1.1/distro-source/core/src/gwt-module.dtd">
<module>
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.i18n.I18N"/>
	<inherits name='com.smartgwt.SmartGwt'/>
	<inherits name="org.geomajas.GeomajasSldSmartGwt"/>  <!-- using SmartGwt -->
	<inherits name="org.geomajas.GeomajasSldApi"/>
	<inherits name="org.geomajas.GeomajasSldCommonGwt"/> 
	<inherits name="org.geomajas.gwt.GeomajasClient"/>
	
	<!-- Plugin Enterprise theme: -->
	<inherits name="org.geomajas.widget.utility.theme.enterprise.EnterpriseResources" />
	
	<extend-property name="locale" values="nl,en" />
	
	<source path="client" />

<!-- set target browser to compile for, use this to limit to the browser used for testing -->
	<!-- where value = "ie6/opera/gecko1_8/safari/gecko" , "gecko1_8" is FireFox 3 -->

	<set-property name="user.agent" value="ie6,opera,gecko1_8,safari" />

	
	<entry-point
		class="org.geomajas.sld.editor.client.SldEditorEntryPoint" />
		
</module>