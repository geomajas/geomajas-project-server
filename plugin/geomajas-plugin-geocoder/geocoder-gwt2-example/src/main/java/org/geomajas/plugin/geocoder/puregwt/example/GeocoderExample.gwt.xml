<module rename-to="example">
	<inherits name="org.geomajas.gwt2.example.base.ExampleBase"/>
	<inherits name='org.geomajas.plugin.geocoder.puregwt.example.GeoCoderExampleJar' />
	<entry-point class="org.geomajas.plugin.geocoder.puregwt.example.client.GeocoderExample" />

	<!-- set target browser to compile for, use this to limit to the browser used for testing -->
	<!-- where value = "ie6/opera/gecko1_8/safari/gecko" , "gecko1_8" is FireFox 3 -->
	<set-property name="user.agent" value="gecko1_8" />

	<source path='client' />
</module>