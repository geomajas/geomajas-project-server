<!-- @extract-start GwtModule, GWT module include geocoder -->
<module rename-to="GeocoderExample">
	<!-- @extract-skip-start -->
	<inherits name="org.geomajas.plugin.geocoder.gwt.example.GeocoderExample"/>
	<!-- @extract-skip-end -->
	<inherits name="com.smartgwt.tools.SmartGwtTools"/>
	<inherits name="org.geomajas.smartgwt.GeomajasClient"/>
	<inherits name="org.geomajas.plugin.geocoder.Geocoder"/>
	<inherits name="com.smartclient.theme.enterprise.Enterprise"/>

	<!-- @extract-skip-start -->

	<!-- add required languages
	<extend-property name="locale" values="nl"/>
	-->

	<!-- set target browser to compile for, use this to limit to the browser used for testing -->
	<!-- where value = "ie6/opera/gecko1_8/safari/gecko" , "gecko1_8" is FireFox 3 -->
	<set-property name="user.agent" value="gecko1_8" />
	<!-- @extract-skip-end -->

	<entry-point class="org.geomajas.plugin.geocoder.gwt.example.client.GeocoderExampleStandalone"/>
</module>
<!-- @extract-end -->